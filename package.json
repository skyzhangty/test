{
  "name": "list-editor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "env": "dev",
    "web_dev_server_port": 3000
  },
  "scripts": {
    "clean": "node node_modules/rimraf/bin.js dist",
    "build": "npm run clean && npm run eslint && webpack --env=$npm_package_config_env --progress --colors",
    "build:offline": "npm run build --list-editor:env=offline",
    "build:dev": "npm run build --list-editor:env=dev",
    "build:test": "npm run build --list-editor:env=test && npm run plato --list-editor:env=test",
    "build:prod": "npm run build --list-editor:env=prod -- -p && npm run plato --list-editor:env=prod",
    "start": "npm run build --list-editor:env=$npm_package_config_env && NODE_ENV=$npm_package_config_env node ./server.js",
    "start:dev": "npm run start --list-editor:env=dev",
    "start:offline": "npm run start --list-editor:env=offline",
    "start:proxy": "npm run start --list-editor:env=proxy",
    "eslint": "./node_modules/.bin/eslint src/scripts/**/*.js test/**/*.js",
    "plato": "./plato.sh $npm_package_config_env",
    "test": "npm run eslint && jest --testPathPattern ./test --coverage --setupTestFrameworkScriptFile=./test/setup.js",
    "testSingle": "npm run eslint && jest --setupTestFrameworkScriptFile=./test/setup.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "qs": "^6.5.1",
    "react": "^15.6.1",
    "react-bootstrap-typeahead": "^2.0.0-alpha.3",
    "react-dom": "^15.6.1",
    "react-modal": "^2.3.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.5",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "route-recognizer": "^0.3.3",
    "url-loader": "0.5.9"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "backend-middleware": "https://github.com/michr-cri/backend-middleware#master",
    "body-parser": "1.17.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "font-awesome": "4.7.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.1.0",
    "jsdom": "^11.1.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "open": "0.0.5",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "rimraf": "2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "3.5.5",
    "webpack-config": "^7.2.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "globals": {
      "APP_URL": "app_url/"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
